#!/bin/bash

source /usr/local/share/Git-Redmine-Suite/helpers/current_git_command
function help_project_remove
{
    echo "$CURRENT_GIT_COMMAND PROJECT"
    exit 1
}

if [ -n "$HELP" ]
then
    help_project_remove
fi

set -e

source /usr/local/share/Git-Redmine-Suite/helpers/redmine-checkconf

PROJECT=$1

if [ -z "$PROJECT" ]
then
    help_project_remove
fi

echo "Remove project $PROJECT ..."
git config --remove-section redmine.project."$PROJECT".finish.user.id
echo ""

