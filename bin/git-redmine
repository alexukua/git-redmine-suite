#!/bin/bash

MODULE="$0-$1"
set -e

if [ -x "$MODULE" ]
then
    shift
    exec $MODULE $*
else
    if [ "x$HELP" == "x" ]
    then
        if [ "x$1" != "x" ]
        then
            echo "The file $MODULE is not present. Check your command"
        fi
        echo ""
        echo "The available command : "
        echo ""
        set +e
        for MODULE in $0-*
        do
            HELP=1 $MODULE
        done
        echo ""
    fi
    exit 1
fi
